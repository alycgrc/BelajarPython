hari_ini = "Minggu"
if (hari_ini == "Senin") :
    print("Saya Pergi Sekolah")
elif (hari_ini == "Selasa") :
     print("Saya Pergi Sekolah")
elif (hari_ini == "Rabu") :
     print("Saya Pergi Less")
elif (hari_ini == "Kamis") :
     print ("Saya Pergi Ekskul")
elif (hari_ini == "Jumat") :
     print("Saya Pergi Ke Rumah Nenek")
elif (hari_ini == "Sabtu") :
     print ("Saya Family Time Di Rumah")
elif (hari_ini == "Minggu") :
     print("Saya Worshiping")

print(hari_ini)
 
